{
  "name": "ipals",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "both": "react-native run-ios && react-native run-android",
    "androidBuild": "cd android && ./gradlew assembleRelease",
    "androidAdb": "cd android && ./gradlew bundleRelease",
    "iosInstall": "cd ios && pod install",
    "gradlewClean": "cd android && ./gradlew clean",
    "runSpecSimulator": "npx react-native run-ios --simulator='iPad Pro (12.9-inch) (5th generation)'",
    "RC": "npx react-native start --reset-cache",
    "resetCache": "npx react-native start --reset-cache",
    "gitPush": "git push -u origin DevOnePro"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/datetimepicker": "^6.5.3",
    "@react-native-picker/picker": "^2.4.8",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@reduxjs/toolkit": "^1.8.6",
    "moment": "^2.29.4",
    "react": "18.1.0",
    "react-native": "0.70.2",
    "react-native-animatable": "^1.3.3",
    "react-native-axios": "^0.17.1",
    "react-native-calendars": "^1.1290.0",
    "react-native-cli": "^2.0.1",
    "react-native-flash-message": "^0.3.1",
    "react-native-image-picker": "^4.10.0",
    "react-native-indicators": "^0.17.0",
    "react-native-modal-datetime-picker": "^14.0.0",
    "react-native-paper": "^4.12.5",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
